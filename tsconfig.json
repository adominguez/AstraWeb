{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@icons/*": [
        "src/icons/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@ts/*": [
        "src/ts/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@types": [
        "src/types.ts"
      ],
      "@email/*": [
        "src/email/*"
      ],
      "@react-mail/*": [
        "react-email-starter/emails/*"
      ],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strictNullChecks": true
  }
}